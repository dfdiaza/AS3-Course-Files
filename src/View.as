package src{	import flash.events.Event;	import flash.events.EventDispatcher;		public class View extends EventDispatcher	{		private var _model:Model;				public function View()		{					}				public function update(event:Event = null):void		{					}				public function set model(m:Model):void		{			_model = m;			_model.addEventListener(Model.MODEL_CHANGE, update);		}				public function get model():*//This way the view class can be used with any model.		{			return _model;		}	}}